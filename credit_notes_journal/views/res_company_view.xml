<odoo>
  <data>

    <record id="view_company_form_accounting" model="ir.ui.view">
        <field name="name">view_company_form_accountinge</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="Accounting">
                    <group>
                        <field name="enable_credit_note_registrations"/>
                    </group>
                </page>
            </notebook>
        </field>
    </record>

  </data>
</odoo>
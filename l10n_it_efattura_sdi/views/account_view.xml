<odoo>
  <data>

        <record id="view_invoice_in_form_sdi" model="ir.ui.view">
            <field name="name">account.invoice.fatturapa.in.sdi</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="l10n_it_fatturapa_in.view_invoice_form_fatturapa_in"></field>
            <field name="arch" type="xml">
                <xpath expr="//page/group/group/field[@name='fatturapa_attachment_in_id']" position="after">
                    <field name="sdi_id" readonly="True" />
                    <field name="sdi_date" readonly="True" />
                </xpath>
            </field>
        </record>

        <record id="view_invoice_out_form_sdi" model="ir.ui.view">
            <field name="name">account.invoice.fatturapa.out.sdi</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="l10n_it_fatturapa_out.view_invoice_form_fatturapa"></field>
            <field name="arch" type="xml">
                <field name="fatturapa_attachment_out_id" position="after">
                    <field name="sdi_state" readonly="True" />
                </field>
            </field>
        </record>

    </data>
</odoo>
